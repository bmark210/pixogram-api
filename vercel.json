{
    "version": 2,
    "buildCommand": "vercel-build",
    "builds": [
       {
        "src": "src/main.ts",
        "use": "@vercel/node",
        "config": {
            "prisma": true
          }
       }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/main.ts",
            "methods": ["GET", "HEAD", "POST", "PUT", "PATCH", "DELETE"]
        }
    ]
}